{
  "name": "os-today",
  "version": "0.0.1",
  "description": "My react seed project",
  "scripts": {
    "start": "heroku local -f Procfile.local",
    "build": "npm run build-client && npm run build-server",
    "build-client": "webpack --config ./tools/webpack.prod",
    "build-server": "rimraf lib && babel ./server -d lib --copy-files",
    "heroku-postbuild": "npm run build",
    "test": "rimraf lib && NODE_ENV=test ./node_modules/mocha/bin/mocha --compilers js:babel-core/register --require babel-polyfill --recursive",
    "test:watch": "npm test -- --watch",
    "coverage": "nyc npm test",
    "lint": "standard"
  },
  "keywords": [],
  "author": "@jaredpalmer",
  "license": "MIT",
  "dependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "autoprefixer": "^6.3.6",
    "babel-cli": "6.10.1",
    "babel-core": "6.13.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "6.5.0",
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.1",
    "compression": "^1.6.2",
    "connect-redis": "^3.1.0",
    "css-loader": "^0.23.1",
    "dotenv": "^2.0.0",
    "errorhandler": "^1.4.3",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "0.9.0",
    "github": "^5.0.1",
    "helmet": "^2.1.0",
    "isomorphic-fetch": "2.2.1",
    "jackrabbit": "^4.3.0",
    "json-loader": "0.5.4",
    "logfmt": "^1.2.0",
    "morgan": "^1.7.0",
    "neo4j": "2.0.0-RC2",
    "passport": "^0.3.2",
    "passport-github2": "^0.1.10",
    "postcss-loader": "^0.11.1",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.0",
    "redux": "^3.5.2",
    "redux-auth-wrapper": "^0.7.0",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.5.2",
    "sanitize.css": "^4.0.0",
    "serialize-javascript": "^1.3.0",
    "style-loader": "^0.13.1",
    "superagent": "^2.2.0",
    "throng": "^4.0.0",
    "url-loader": "0.5.7",
    "webpack": "1.13.1"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "babel-plugin-istanbul": "^1.0.3",
    "babel-register": "^6.9.0",
    "expect": "^1.20.1",
    "mocha": "^3.0.1",
    "nyc": "^8.1.0",
    "redbox-react": "^1.2.9",
    "standard": "^7.1.2",
    "standard-loader": "^5.0.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false,
    "exclude": [
      "**/migrations/**",
      "**/tools/**",
      "**/test/**"
    ]
  },
  "engines": {
    "node": "6.2.2"
  }
}
